{
  "name": "@feedapp/root",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "serve": "nx serve app --port 8080",
    "build": "nx build app",
    "test": "nx run-many -t test --parallel 10",
    "lint": "nx run-many -t lint,stylelint --parallel 10 && nx format:check --all",
    "lint:fix": "nx run-many -t lint:fix,stylelint:fix --parallel 10 && nx format:write --all",
    "storybook": "nx storybook app",
    "build-storybook": "nx run-many -t build-storybook --parallel 10",
    "kill": "lsof -i :8080 | awk 'FNR == 2 {print $2}' | xargs kill -9"
  },
  "private": true,
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.3.2",
    "@nx/js": "16.3.2",
    "@nx/linter": "16.3.2",
    "@nx/storybook": "^16.3.2",
    "@nx/workspace": "16.3.2",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-styling": "^1.3.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.15.1",
    "nx": "16.3.2",
    "nx-stylelint": "^15.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.53.0",
    "stylelint": "^15.9.0",
    "stylelint-config-standard": "^33.0.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsconfig": "^7.0.0",
    "tslib": "^2.5.3",
    "typescript": "~5.1.6",
    "vue-eslint-parser": "^9.3.1"
  },
  "workspaces": [
    "packages/*",
    "app"
  ],
  "dependencies": {
    "code-tag": "^1.1.0"
  }
}
