{
  "name": "feedapp",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "serve:app": "nx serve app --port 8080",
    "serve:backend": "nx serve backend --port 3005",
    "build": "nx run-many -t build --parallel 10",
    "test": "nx run-many -t test --parallel 10",
    "lint": "nx run-many -t lint,stylelint --parallel 10 && nx format:check --all",
    "lint:fix": "nx run-many -t lint:fix,stylelint:fix --parallel 10 && nx format:write --all",
    "storybook": "nx storybook app",
    "build-storybook": "nx run-many -t build-storybook --parallel 10",
    "kill": "lsof -i :8080 | awk 'FNR == 2 {print $2}' | xargs kill -9"
  },
  "private": true,
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.3.2",
    "@nx/nest": "^16.10.0",
    "@nx/node": "16.10.0",
    "@nx/storybook": "^16.3.2",
    "@nx/webpack": "16.10.0",
    "@nx/workspace": "16.3.2",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-styling": "^1.3.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.15.1",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "16.3.2",
    "nx-stylelint": "^15.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.69.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "supabase": "^1.100.1",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsconfig": "^7.0.0",
    "tslib": "^2.5.3",
    "typescript": "~5.1.6",
    "vue-eslint-parser": "^9.3.1"
  },
  "workspaces": [
    "packages/*",
    "app"
  ],
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^10.0.0",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
